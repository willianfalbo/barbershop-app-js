{
  "name": "barbershop-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "ts-node-dev --transpile-only --ignore node_modules src/server.ts",
    "watch:debug": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "docker:db": "docker run --name barbershop-postgres -p 5432:5432 -e POSTGRES_PASSWORD=password -d postgres",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:add": "yarn typeorm migration:create -n ",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.3.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  }
}
